<html t:type="layout" title="Prikazi Zaposlene"
	 xmlns:p="tapestry:parameter"
        xmlns:j="tapestry-library:jquery"
  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
 >

     <t:grid source="zaposleniSource"  
           rowsPerPage="5" row="zaposleni" >
           <p:prezimeCell>
           	<t:pageLink t:page="about" t:context="zaposleni.idZaposlenog">
           		${zaposleni.prezime}
           	</t:pageLink>
           </p:prezimeCell>
            <p:deleteCell>
           	<t:pageLink t:page="about" t:context="zaposleni.idZaposlenog">
           		delete
           	</t:pageLink>
           </p:deleteCell>
       
      
	</t:grid>
   
	

</html>
